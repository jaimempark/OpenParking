<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">



    <TextView
        android:id="@+id/viewAddress"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:text="Address"

        android:textAlignment="center" />

    <TextView
        android:id="@+id/viewZipCode"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="30dp"

        android:text="ZipCode"

        android:textAlignment="center" />

    <TextView
        android:id="@+id/viewLatitude"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="60dp"

        android:text="Latitude"

        android:textAlignment="center" />

    <TextView
        android:id="@+id/viewLongitude"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="90dp"

        android:text="Longitude"

        android:textAlignment="center" />
    <TextView
        android:id="@+id/viewCost"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="120dp"

        android:text="Cost"

        android:textAlignment="center" />

    <TextView
        android:id="@+id/viewOpenTime"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="150dp"

        android:text="OpenTime"

        android:textAlignment="center" />

    <TextView
        android:id="@+id/viewCloseTime"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="180dp"

        android:text="CloseTime"

        android:textAlignment="center" />

    <TextView
        android:id="@+id/viewOwner"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/avatar"
        android:paddingLeft="120dp"
        android:paddingRight="110dp"
        android:paddingBottom="10dp"
        android:paddingTop="210dp"

        android:text="OwnerName"

        android:textAlignment="center" />
</android.support.constraint.ConstraintLayout>